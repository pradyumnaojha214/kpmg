    "def challenge3(test_dict,key):\n",
    "    key_list = key.split(\"/\")\n",
    "    res_dict = test_dict\n",
    "    for key in key_list:\n",
    "        res_dict = res_dict[key]\n",
    "    return res_dict\n",
    "\n",
    "print(challenge3({\"a\":{\"b\":{\"c\":\"d\"}}}, \"a/b/c\"))\n",
    "print(challenge3({\"x\":{\"y\":{\"z\":\"a\"}}}, \"x/y/z\"))\n"
   ]
